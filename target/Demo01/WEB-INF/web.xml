<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.luoyingmm.java.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>VIP1</servlet-name>
    <servlet-class>com.luoyingmm.java.VIP1out</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VIP1</servlet-name>
    <url-pattern>/servlet/VIP1out</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>VIP2</servlet-name>
    <servlet-class>com.luoyingmm.java.VIP2out</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VIP2</servlet-name>
    <url-pattern>/servlet/VIP2out</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>VIP3</servlet-name>
    <servlet-class>com.luoyingmm.java.VIP3out</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VIP3</servlet-name>
    <url-pattern>/servlet/VIP3out</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>VIP4</servlet-name>
    <servlet-class>com.luoyingmm.java.VIP4out</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VIP4</servlet-name>
    <url-pattern>/servlet/VIP4out</url-pattern>
  </servlet-mapping>

  <filter>
    <filter-name>FilterLogin</filter-name>
    <filter-class>com.luoyingmm.filter.FilterLogin</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>FilterLogin</filter-name>
    <url-pattern>/servlet/*</url-pattern>
  </filter-mapping>
</web-app>
